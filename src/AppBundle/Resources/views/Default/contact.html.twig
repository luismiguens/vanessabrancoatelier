
      <!-- Contact Section -->
      <section id="Contact">
         <div class="container">
            <div class="row">
               <div class="section-title center wow fadeIn">
                  <h2>{% trans %}contact_say_hello{% endtrans %}</h2>
                  <div class="line">
                  </div>
                  <p>{% trans %}contact_intro{% endtrans %}</p>
               </div>
               <div class="col-md-7 contactLeft">
                  <!-- contact FORM-->
                  <div class="contactForm">
                     <!--MAIN FORM-->
                     <form name="sentMessage" id="contactForm" novalidate>
                        <div class="form-group">
                           <input type="text" class="form-control" placeholder="Name* :" id="name" required data-validation-required-message="Please enter your name.">
                           <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                           <input type="email" class="form-control" placeholder="E-mail* :" id="email" required data-validation-required-message="Please enter your email address.">
                           <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                           <input type="tel" class="form-control" placeholder="Subject* :" id="phone" required data-validation-required-message="Please enter your phone number.">
                           <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                           <textarea class="form-control" placeholder="Message* :" id="message" rows="3" cols="40" required data-validation-required-message="Please enter a message."></textarea>
                           <p class="help-block text-danger"></p>
                        </div>
                        <div id="success"></div>
                        <button type="submit" class="submit wow bounceIn">Send Message</button>
                     </form>
                     <!--END Main form-->
                  </div>
                  <!-- END contact FORM-->
               </div>
               <div class="col-md-5 contactRight">
                  {#<div class="contacDetails wow fadeInRight" data-wow-duration="1s" data-wow-delay="0.2s">
                     <h3>{% trans %}contact_contact_details{% endtrans %}</h3>
                     <p>{% trans %}contact_contact_intro{% endtrans %} </p>
                  </div>#}
                  <div class="contacDetails wow fadeInRight" data-wow-duration="1s" data-wow-delay="0.4s">
                     <div class="icon icon-map"></div>
                     <h3>{% trans %}contact_contact_address{% endtrans %}</h3>
                     <p>Rua dos Mercadores, 63, Gabinete 4
 <br/>
9500-092 Ponta Delgada, Azores, Portugal</p>
                  </div>
                  <div class="contacDetails wow fadeInRight" data-wow-duration="1s" data-wow-delay="0.6s">
                     <div class="icon icon-envelope"></div>
                     <h3>{% trans %}contact_contact_email_us{% endtrans %}</h3>
                     <p>branco.vanessa.atelier@gmail.com</p>
                  </div>
               </div>
            </div>
         </div>
         <div id="map"></div>
      </section>